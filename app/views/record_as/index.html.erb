<p id="notice"><%= notice %></p>

<h1>Listing Record As</h1>

<table>
  <thead>
    <tr>
      <th>Attr1</th>
      <th>Attr2</th>
      <th>Attr3</th>
      <th>Attr4</th>
      <th>Attr5</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @record_as.each do |record_a| %>
      <tr>
        <td><%= record_a.attr1 %></td>
        <td><%= record_a.attr2 %></td>
        <td><%= record_a.attr3 %></td>
        <td><%= record_a.attr4 %></td>
        <td><%= record_a.attr5 %></td>
        <td><%= link_to 'Show', record_a %></td>
        <td><%= link_to 'Edit', edit_record_a_path(record_a) %></td>
        <td><%= link_to 'Destroy', record_a, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Record a', new_record_a_path %>
